<apex:page standardController="Ads_Reference_By__c" recordSetVar="Add_Reference_Bys" >
    <apex:form >
    <apex:pageBlock >
        <apex:pageBlockSection title="Add Ref By">
            <apex:inputField value="{!Ads_Reference_By__c.Last_Name__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.First_Name__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.Middle_Name__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.Mobile_No__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.Address__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.Landline_No__c}"/>
            <apex:inputField value="{!Ads_Reference_By__c.Degree__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton action="{!save}" value="Save"/>
              <apex:commandButton action="{!cancel}" value="Cancel"/>
             </apex:pageBlockButtons>
        
        </apex:pageBlock>

    
    </apex:form>
</apex:page>